<RelativeLayout android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <ImageView
        android:id="@+id/high_score_level2_id"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scaleType="centerCrop"
        android:src="@drawable/background_guapo_game_startscreen2" />

    <ImageView
        android:id="@+id/volumeCtrl"
        android:layout_width="36dp"
        android:layout_height="36dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginTop="23dp"
        android:layout_marginEnd="28dp"
        android:layout_marginRight="28dp"
        android:src="@drawable/volume_on"
        app:layout_constraintEnd_toEndOf="@+id/high_score_level2_id"
        app:layout_constraintTop_toTopOf="@+id/high_score_level2_id" />

    <Button
        android:id="@+id/level1_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="266dp"
        android:layout_marginLeft="266dp"
        android:layout_marginTop="104dp"
        android:backgroundTint="@android:color/holo_blue_dark"
        android:text="mondi"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/high_score_level2_id"
        app:layout_constraintHorizontal_bias="0.442"
        app:layout_constraintStart_toStartOf="@+id/high_score_level2_id"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.211" />

    <Button
        android:id="@+id/second_level2_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/level1_id"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_marginStart="267dp"
        android:layout_marginLeft="267dp"
        android:layout_marginTop="10dp"
        android:backgroundTint="@android:color/holo_blue_dark"
        android:text="beach"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.443"
        app:layout_constraintStart_toStartOf="@+id/high_score_level2_id"
        app:layout_constraintTop_toBottomOf="@+id/level1_id"
        app:layout_constraintVertical_bias="0.054" />

    <Button
        android:id="@+id/trip_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/second_level2_id"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_marginStart="268dp"
        android:layout_marginLeft="268dp"
        android:layout_marginTop="11dp"
        android:backgroundTint="@android:color/holo_blue_dark"
        android:text="trip    "
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="@+id/high_score_level2_id"
        app:layout_constraintEnd_toEndOf="@+id/high_score_level2_id"
        app:layout_constraintHorizontal_bias="0.444"
        app:layout_constraintStart_toStartOf="@+id/high_score_level2_id"
        app:layout_constraintTop_toBottomOf="@+id/second_level2_id"
        app:layout_constraintVertical_bias="0.088" />

    <Button
        android:id="@+id/ocean_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/trip_id"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_marginStart="268dp"
        android:layout_marginLeft="268dp"
        android:layout_marginTop="12dp"
        android:backgroundTint="@android:color/holo_blue_dark"
        android:text="ocean"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="@+id/high_score_level2_id"
        app:layout_constraintEnd_toEndOf="@+id/high_score_level2_id"
        app:layout_constraintHorizontal_bias="0.447"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/trip_id"
        app:layout_constraintVertical_bias="0.097" />

    <TextView
        android:id="@+id/high_score_id_level1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginStart="52dp"
        android:layout_marginLeft="52dp"
        android:layout_marginTop="114dp"
        android:layout_marginEnd="298dp"
        android:layout_marginRight="298dp"
        android:layout_toEndOf="@+id/level1_id"
        android:layout_toRightOf="@+id/level1_id"
        android:text="0"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="@+id/high_score_level2_id"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.168"
        app:layout_constraintStart_toEndOf="@+id/level1_id"
        app:layout_constraintTop_toTopOf="@+id/high_score_level2_id"
        app:layout_constraintVertical_bias="0.226" />

    <TextView
        android:id="@+id/high_score_id_level2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/high_score_id_level1"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginStart="51dp"
        android:layout_marginLeft="51dp"
        android:layout_marginTop="27dp"
        android:layout_marginEnd="299dp"
        android:layout_marginRight="299dp"
        android:layout_toEndOf="@+id/second_level2_id"
        android:layout_toRightOf="@+id/second_level2_id"
        android:text="0"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/high_score_level2_id"
        app:layout_constraintHorizontal_bias="0.168"
        app:layout_constraintStart_toEndOf="@+id/second_level2_id"
        app:layout_constraintTop_toBottomOf="@+id/high_score_id_level1"
        app:layout_constraintVertical_bias="0.114" />

    <TextView
        android:id="@+id/high_score_id_level3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/high_score_id_level2"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginStart="52dp"
        android:layout_marginLeft="52dp"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="299dp"
        android:layout_marginRight="299dp"
        android:layout_toEndOf="@+id/trip_id"
        android:layout_toRightOf="@+id/trip_id"
        android:text="0"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="@+id/high_score_level2_id"
        app:layout_constraintEnd_toEndOf="@+id/high_score_level2_id"
        app:layout_constraintHorizontal_bias="0.168"
        app:layout_constraintStart_toEndOf="@+id/trip_id"
        app:layout_constraintTop_toBottomOf="@+id/high_score_id_level2"
        app:layout_constraintVertical_bias="0.185" />

    <TextView
        android:id="@+id/high_score_id_level_4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/high_score_id_level3"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginStart="48dp"
        android:layout_marginLeft="48dp"
        android:layout_marginTop="33dp"
        android:layout_marginEnd="299dp"
        android:layout_marginRight="299dp"
        android:layout_toEndOf="@+id/ocean_id"
        android:layout_toRightOf="@+id/ocean_id"
        android:text="0"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="@+id/high_score_level2_id"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.158"
        app:layout_constraintStart_toEndOf="@+id/ocean_id"
        app:layout_constraintTop_toBottomOf="@+id/high_score_id_level3"
        app:layout_constraintVertical_bias="0.2" />

</RelativeLayout>


